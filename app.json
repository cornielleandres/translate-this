{
  "expo": {
    "name": "Translate This!",
    "slug": "translate-this",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "host.exp.translatethis",
      "versionCode": 1
    },
    "ios": {
      "buildNumber": "1.0.0",
      "bundleIdentifier": "host.exp.translatethis",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take pictures of whatever object for which the user wants a translation."
      },
      "supportsTablet": true
    }
  }
}